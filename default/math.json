{
  "agent_role": "ci_workflow_16generator",
  "repository": {
    "url": "https://github.com/lakshmi1212/Example105",
    "default_branch": "main",
    "feature_branch": "Feature1"
  },
  "workflow": {
    "file_path": ".github/workflows/ci.yml",
    "name": "CI Pipeline"
  },
  "triggers": {
    "push": ["Feature1"],
    "pull_request": ["main"]
  },
  "runtime": {
    "language": "python",
    "version": "3.10"
  },
  "dependencies": [
    "pytest==8.0.0",
    "pytest-html==4.1.1"
  ],
  "test_execution": {
    "test_directory": "tests",
    "pytest_command": "pytest tests/",
    "fail_on_error": true
  },
  "test_reports": {
    "enabled": true,
    "report_directory": "reports",
    "formats": [
      "junit",
      "html"
    ],
    "junit_file": "reports/report.xml",
    "html_file": "reports/report.html"
  },
  "s3_upload": {
    "enabled": false,
    "bucket_name": "",
    "prefix": "",
    "region": "",
    "versioning_strategy": ""
  },
  "constraints": {
    "modify_source_files": false,
    "modify_test_files": false
  },
  "commit": {
    "enabled": true,
    "message": "Add CI workflow for Python tests"
  },
  "jobs": [
    {
      "job_id": "ci_test",
      "job_name": "Run Pytest and Generate Reports",
      "runs_on": "ubuntu-latest",
      "steps": [
        {
          "step_name": "Checkout Repository",
          "action_type": "uses",
          "action": "actions/checkout@v4"
        },
        {
          "step_name": "Set up Python",
          "action_type": "uses",
          "action": "actions/setup-python@v5",
          "with": {
            "python-version": "3.10"
          }
        },
        {
          "step_name": "Install dependencies",
          "action_type": "run",
          "command": "pip install -r default/requirements.txt"
        },
        {
          "step_name": "Run Pytest",
          "action_type": "run",
          "command": "pytest tests/ --junitxml=reports/report.xml --html=reports/report.html --self-contained-html"
        }
      ]
    }
  ]
}
